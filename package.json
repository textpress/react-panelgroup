{
  "name": "@textpress/react-panelgroup",
  "version": "1.0.5",
  "description": "react-panelgroup React component",
  "main": "lib/PanelGroup.js",
  "jsnext:main": "es/PanelGroup.js",
  "module": "es/PanelGroup.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && npm clean-demo",
    "start": "nwb serve-react-demo",
    "test": "nwb test",
    "test:coverage": "nwb test --coverage",
    "test:watch": "nwb test --server",
    "deploy": "gh-pages -d demo/dist",
    "bump": "gulp bump --part",
    "ci-build": "yarn build",
    "ci-test": "yarn test",
    "ci-release-notes": "conventional-github-releaser -p eslint"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@textpress/gulp-bump-version": "^0.2.2",
    "conventional-github-releaser": "^2.0.0",
    "gh-pages": "^0.11.0",
    "gulp": "^3.9.1",
    "nwb": "^0.20.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "contributors": [
    "Dan Fessler",
    "Textpress"
  ],
  "license": "MIT",
  "repository": "https://github.com/textpress/react-panelgroup",
